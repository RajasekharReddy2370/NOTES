#####################################################################CREATE NEW REPO using CODE#########################################################################


gh auth login
gh repo create Encode_Decode --public --source=. --remote=origin
git push -u origin main

#####################################################################After creating repo in Git_HUB######################################################
1.cd /path/to/your/project
2.git init
3.git add .

4.git commit -m "Initial commit"
5.git remote add origin https://github.com/yourusername/your-repository-name.git
6.git remote add origin https://github.com/RajasekharReddy2370/Encode_Decode.git
7.git push -u origin main


git checkout branch_name (To change branch name)

git help -a "To see all commands"

git stash



